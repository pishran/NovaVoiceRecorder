{
    "name": "pishran/nova-voice-recorder",
    "description": "A Laravel Nova field.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Pishran\\NovaVoiceRecorder\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Pishran\\NovaVoiceRecorder\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
